#include<iostream>
#include<cmath>
using namespace std;
void findQuad(double  a,double b,double c){
   double d=b*b-4*a*c;
    if(d>0){
       double r1=(-b+sqrt (d))/(2*a);
       double r2=(-b- sqrt(d))/(2*a);
        cout<<"real and different root r1: "<< r1 << ", r2 : " <<r2;
    }else if(d==0){
       double  r=-b/(2*a);
        cout<<"equal rool"<<r;
    }else if(d<0){
     double  r3= -b/(2*a);
      double r4=(-b+sqrt(d))/(2*a);
      cout<<r3;
      cout<<r4;
    }

}
int main(){
    double a=1,b=3,c=1,r,r1,r2,r3,r4,d;
    if(a==0){
        cout<<"invalide";
    }else{
      findQuad(a,b,c);
    }
    return 0;
    
}
